{
  "name": "reactish-boilerplate",
  "version": "0.1.0",
  "description": "A Webpack, React, Redux and Sass (and more) boilerplate",
  "private": true,
  "scripts": {
    "start": "clear && webpack-dev-server",
    "clean": "rimraf build",
    "lint": "eslint src || true",
    "test": "clear && jest ./tests --setupTestFrameworkScriptFile=raf/polyfill",
    "build": "npm run clean && webpack --config webpack.production.config.js --progress --profile --colors"
  },
  "author": "Patrick Davidson",
  "homepage": "https://github.com/pacdavidson/reactish-boilerplate",
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.9",
    "dotenv-webpack": "^1.5.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "node-sass": "^4.7.2",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-loader": "^2.0.10",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.0"
  },
  "dependencies": {
    "prop-types": "^15.6.1",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "react-hot-loader": "^3.1.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-promise-middleware": "^5.0.0"
  }
}
